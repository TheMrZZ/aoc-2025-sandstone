{
  "compilerOptions": {
    "declaration": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "es2024",
    "lib": [
      "es2024"
    ],
    "strict": false,
    "esModuleInterop": false,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": false,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "outDir": "build"
  }
}